meta {
  name: Henter oversigt over test resultater
  type: http
}

get {
  url: {{baseUrl}}/api/BrunoTestResults/overview
  body: none
  auth: none
}

headers {
  Content-Type: application/json
}

tests {
  test("Status kode skal være 200 OK", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response skal være et objekt", function() {
    expect(res.getBody()).to.be.an('object');
  });
  
  test("Response skal have AvailableResults array", function() {
    expect(res.getBody().AvailableResults).to.be.an('array');
  });
  
  test("Response skal have LatestJsonResult og LatestHtmlResult", function() {
    expect(res.getBody()).to.have.property('LatestJsonResult');
    expect(res.getBody()).to.have.property('LatestHtmlResult');
  });
  
  test("Response tid skal være under 1 sekund", function() {
    expect(res.getResponseTime()).to.be.below(1000);
  });
}
